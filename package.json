{
  "name": "stylelint-config-standard-vue",
  "version": "1.0.0",
  "description": "The standard shareable Vue config for Stylelint.",
  "keywords": [
    "stylelint",
    "stylelint-config",
    "standard",
    "vue"
  ],
  "main": "lib/index.js",
  "files": [
    "lib",
    "scss"
  ],
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "test": "mocha \"tests/lib/**/*.js\" --reporter dot --timeout 60000",
    "lint": "eslint .",
    "eslint-fix": "eslint . --fix",
    "preversion": "npm test && git add ."
  },
  "dependencies": {
    "stylelint-config-html": ">=1.0.0",
    "stylelint-config-recommended-vue": ">=1.1.0",
    "stylelint-config-standard": ">=24.0.0"
  },
  "peerDependencies": {
    "postcss-html": "^1.0.0",
    "stylelint": ">=14.0.0"
  },
  "devDependencies": {
    "@eslint-community/eslint-plugin-eslint-comments": "^4.3.0",
    "@ota-meshi/eslint-plugin": "^0.20.0",
    "eslint": "^10.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-jsdoc": "^62.4.0",
    "eslint-plugin-json-schema-validator": "^6.0.0",
    "eslint-plugin-jsonc": "^3.0.0",
    "eslint-plugin-n": "^17.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-regexp": "^3.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-plugin-yml": "^3.0.0",
    "mocha": "^11.0.0",
    "prettier": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ota-meshi/stylelint-config-standard-vue.git"
  },
  "author": "Yosuke Ota (https://github.com/ota-meshi)",
  "funding": "https://github.com/sponsors/ota-meshi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ota-meshi/stylelint-config-standard-vue/issues"
  },
  "homepage": "https://github.com/ota-meshi/stylelint-config-standard-vue#readme"
}
